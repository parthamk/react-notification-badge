{
  "name": "react-notification-badge",
  "version": "1.5.1",
  "description": "Simple notification badge react component",
  "main": "lib/index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "yarn run watch:example & yarn run browser",
    "test:local": "karma start",
    "test": "NODE_EVN=test ./node_modules/.bin/karma start --browsers Firefox --single-run && codecov",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mobilusoss/react-notification-badge"
  },
  "keywords": [
    "react",
    "react-component",
    "badge",
    "notification"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobilusoss/react-notification-badge/issues"
  },
  "homepage": "https://github.com/mobilusoss/react-notification-badge",
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babelify": "^10.0.0",
    "browser-sync": "^3.0.2",
    "browserify": "^17.0.0",
    "chai": "^5.1.1",
    "codecov": "^3.8.2",
    "eslint": "^9.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "karma": "^6.4.3",
    "karma-browserify": "^8.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.36",
    "mocha": "^10.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "sinon": "^18.0.0",
    "watchify": "^4.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "dependencies": {
    "core-js": "3"
  }
}
